<body>
  <div class=3D"logo">
    <img src=3D"file:///C:/Users/prett/Downloads/enki-logo-extended.png" al=
t=3D"Enki Logo">
  </div>
  <div class=3D"tagline">Your Path to Smart Home Financing</div>

  <h1>Mortgage Application</h1>

  <div class=3D"form-section">
    <form action=3D"mailto:mortgages@enkifsi.ca" method=3D"POST" enctype=3D=
"text/plain">
      <label>Full Name:</label>
      <input type=3D"text" name=3D"Full Name" required=3D"">

      <label>Email Address:</label>
      <input type=3D"email" name=3D"Email" required=3D"">

      <label>Phone Number:</label>
      <input type=3D"tel" name=3D"Phone" required=3D"">

      <label>Current Address:</label>
      <input type=3D"text" name=3D"Address">

      <label>Application Type:</label>
      <select name=3D"Application Type">
        <option>Purchase</option>
        <option>Refinance</option>
        <option>Home Equity Line of Credit</option>
      </select>

      <label>Employment Status:</label>
      <select name=3D"Employment Status">
        <option>Employed</option>
        <option>Self-Employed</option>
        <option>Unemployed</option>
        <option>Retired</option>
      </select>

      <label>Annual Income ($):</label>
      <input type=3D"number" name=3D"Annual Income">

      <label>Down Payment Available ($):</label>
      <input type=3D"number" name=3D"Down Payment">

      <label>Estimated Purchase Price ($):</label>
      <input type=3D"number" name=3D"Purchase Price">

      <label>Additional Notes:</label>
      <textarea name=3D"Notes" rows=3D"4"></textarea>

      <button type=3D"submit">Submit Application</button>
    </form>
  </div>


</body></html>
